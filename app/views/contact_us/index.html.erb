<div class="container">
  <div class="row">
    <div class="col-md-6">
      <div id="map"></div>
      <script>
        function initMap() {
          var uluru = {lat: -25.363, lng: 131.044};
          var map = new google.maps.Map(document.getElementById('map'), {
            zoom: 4,
            center: uluru
          });
          var marker = new google.maps.Marker({
            position: uluru,
            map: map
          });
        }
      </script>
      <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBgbEi-TP5VMKigppAmN7e7ujen_PGg2ZU&callback=initMap"></script>
    </div>
    <div class="col-md-6">
    </div>
  </div>
</div>